<div data-ng-controller="StudentCtrl">
    <div class="col-xs-6">
        <table class="table table-responsive table-striped">
            <thead>
                <tr>
                    <th>
                        Last Name
                    </th>
                    <th>
                        First Name, MI
                    </th>
                    <th>
                        Enrollment Date
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                <tr data-ng-repeat="student in students">
                    <td>
                        {{student.LastName}}
                    </td>
                    <td>
                        {{student.FirstMidName}}
                    </td>
                    <td>
                        {{student.EnrollmentDate}}
                    </td>
                    <td>
                        <a href="/#/edit/{{student.Id}}">Edit</a> | 
                        <a href="/#/details/{{student.Id}}">Details</a> | 
                        <a href="/#/delete/{{student.Id}}">Delete</a> 
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>